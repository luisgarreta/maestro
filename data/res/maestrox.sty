#workspace_navigator QTreeView,
#maestro_project_table QLineEdit {
    font-family: "DejaVu LGC Sans Mono";
/* <WIN32> */
    font-family: "DejaVu Sans Mono";
/* </WIN32> */
/* <DARWIN> */
    font-family: "Menlo";
/* </DARWIN> */
}
