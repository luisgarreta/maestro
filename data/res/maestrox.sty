    background-color: rgb( 188, 171, 80 );
    border-style: inset;
}
QPushButton[periodicColorIndex="1"]:checked {
    background-color: rgb( 188, 171, 80 );
    border-style: inset;
}


QPushButton[periodicColorIndex="2"] {
    background-color: rgb( 240, 180, 180 );
    border-style: outset;
    border-width: 2px;
    border-radius: 2px;
    border-color: gray;
}

QPushButton[periodicColorIndex="2"]:pressed {
    background-color: rgb( 190, 130, 130 );
    border-style: inset;
}
QPushButton[periodicColorIndex="2"]:checked {
    background-color: rgb( 190, 130, 130 );
    border-style: inset;
}

QPushButton[periodicColorIndex="3"] {
    background-color: rgb( 50, 205, 50 );
    border-style: outset;
    border-width: 2px;
    border-radius: 2px;
    border-color: gray;
}

QPushButton[periodicColorIndex="3"]:pressed {
    background-color: rgb( 5, 155, 5 );
